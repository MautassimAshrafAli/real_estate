<body class="hold-transition sidebar-mini layout-fixed" style="height: auto;background-color: #e9ecef;">


    @RenderSection("login", required: false)

    @*@{string root = @Request.Url.Scheme + "://" + @Request.Url.Authority+"/"; }*@

    @if (TempData["login"] == "done")
